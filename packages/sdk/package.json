  },
  "dependencies": {
    "rimraf": "^3.0.2",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "openapi-typescript": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3"
  }
